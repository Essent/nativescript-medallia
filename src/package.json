{
		"name": "@essent/nativescript-medallia",
		"version": "4.0.0",
		"description": "Nativescript plugin for medallia integration.",
		"main": "medallia",
		"typings": "index.d.ts",
		"nativescript": {
				"platforms": {
						"android": "6.5.0",
						"ios": "6.5.0"
				}
		},
		"scripts": {
				"tsc": "tsc",
				"build": "npm i --legacy-peer-deps && ts-patch install && tsc",
				"demo.ios": "npm run build && cd ../demo && ns run ios",
				"demo.android": "npm run build && cd ../demo && ns run android",
				"demo.reset": "cd ../demo && ns clean",
				"clean": "npm run demo.reset && npx rimraf node_modules package-lock.json && npm run build"
		},
		"keywords": [
				"NativeScript",
				"JavaScript",
				"Android",
				"iOS",
				"Medallia"
		],
		"author": {
				"name": "Essent",
				"email": "frontend-licenties@essent.nl"
		},
		"bugs": {
				"url": "https://github.com/Essent/nativescript-medallia/issues"
		},
		"license": "MIT",
		"homepage": "https://github.com/Essent/nativescript-medallia",
		"readmeFilename": "README.md",
		"devDependencies": {
				"@nativescript/core": "~8.3.0",
				"@nativescript/types": "~8.3.0",
				"@nativescript/webpack": "^5.0.0",
				"prompt": "^1.0.0",
				"rimraf": "^3.0.2",
				"rxjs": "~7.5.0",
				"semver": "^6.2.0",
				"ts-patch": "^1.3.0",
				"ts-node": "^10.9.1",
				"typescript": "~4.7.0"
		},
		"bootstrapper": "nativescript-plugin-seed"
}
